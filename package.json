{
  "name": "yuta9-pege",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "dev": "run-z server, build:css:watch",
    "server": "next dev -p 3333",
    "build": "next build",
    "build:css:watch": "hcm 'src/**/*.module.css' --localsConvention=camelCaseOnly --watch",
    "build:css": "hcm 'src/**/*.module.css' --localsConvention=camelCaseOnly ",
    "start": "next start",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:js": "eslint",
    "lint:js:fix": "eslint --fix",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint:css:fix": "stylelint \"src/**/*.css\" --fix",
    "ts:test": "tsc --noEmit",
    "icons": "npx @svgr/cli --no-index -d src/components/icons public/icons"
  },
  "dependencies": {
    "@csstools/postcss-global-data": "^3.0.0",
    "add": "^2.0.6",
    "chart.js": "^3.9.1",
    "classnames": "^2.5.1",
    "date-fns": "^2.30.0",
    "i18next": "^22.5.1",
    "microcms-js-sdk": "^2.7.0",
    "next": "15.1.7",
    "next-i18next": "^13.3.0",
    "react": "19.0.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "19.0.0",
    "react-i18next": "^12.2.0",
    "react-scroll": "^1.9.0",
    "reset-css": "^5.0.2",
    "swiper": "8.4.6"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@next/eslint-plugin-next": "^15.1.7",
    "@svgr/cli": "^7.0.0",
    "@types/chart.js": "^2.9.41",
    "@types/node": "20.10.3",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@types/react-scroll": "^1.8.10",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "9.20.1",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.15.0",
    "happy-css-modules": "^3.2.0",
    "husky": "^8.0.3",
    "postcss": "^8.5.2",
    "postcss-custom-media": "^11.0.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.1.4",
    "prettier": "^3.5.0",
    "run-z": "^2.1.0",
    "stylelint": "^16.14.1",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard": "^37.0.0",
    "typescript": "5.4.2",
    "typescript-eslint": "^8.24.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.8",
      "@types/react-dom": "19.0.3"
    }
  }
