{
  "editor.bracketPairColorization.enabled": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.addMissingImports": "explicit"
  },
  "stylelint.validate": ["css", "postcss"],
  "stylelint.snippet": ["css", "postcss"],
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "stylelint.vscode-stylelint"
  },
  "[markdown]": {
    "editor.formatOnSave": true
  },
  "search.exclude": {
    "**/node_modules": true
  },
  "cSpell.words": ["Toeic", "Zenn", "Qiita"],
  "peacock.color": "#01ff76",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#34ff91",
    "activityBar.activeBorder": "#e080ff",
    "activityBar.background": "#34ff91",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#ba80ff",
    "activityBarBadge.foreground": "#15202b",
    "sash.hoverBorder": "#34ff91",
    "statusBar.background": "#01ff76",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#00cd5e",
    "statusBarItem.remoteBackground": "#01ff76",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#01ff76",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#01ff7699",
    "titleBar.inactiveForeground": "#15202b99",
    "commandCenter.border": "#15202b99"
  },
  "i18n-ally.localesPaths": ["public/locales"],
  "i18n-ally.keystyle": "nested",
  "biome.enabled": false
}
